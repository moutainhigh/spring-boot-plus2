<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ws.ldy.modules.dev.mapper.DevTaskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ws.ldy.modules.dev.model.entity.DevTask">

        <result column="id" property="id"/>
        <result column="create_time" property="createTime"/>
        <result column="create_user" property="createUser"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="deleted" property="deleted"/>
        <result column="version" property="version"/>
        <result column="task_user_id" property="taskUserId"/>
        <result column="item" property="item"/>
        <result column="name" property="name"/>
        <result column="content" property="content"/>
        <result column="type" property="type"/>
        <result column="state" property="state"/>
        <result column="planned_time" property="plannedTime"/>
        <result column="ent_time" property="entTime"/>
        <result column="estimate_time" property="estimateTime"/>
        <result column="take_up_time" property="takeUpTime"/>
    </resultMap>

    <!-- 通用查询结果列: <include refid="Base_Column_List"></include>  -->
    <sql id="Base_Column_List">

               id,
               create_time,
               create_user,
               update_user,
               update_time,
               deleted,
               version,
               task_user_id,
               item,
               name,
               content,
               type,
               state,
               planned_time,
               ent_time,
               estimate_time,
               take_up_time
        </sql>


    <select id="findList" resultType="com.ws.ldy.modules.dev.model.entity.DevTask">
        select
        <include refid="Base_Column_List"></include>
        from t_dev_task
    </select>


</mapper>

