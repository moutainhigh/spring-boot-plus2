    <properties>
        <java.version>1.8</java.version>
        <!-- 项目/模板版本号管理 -->
<!--        <admin-core.version>0.0.1-SNAPSHOT</admin-core.version>               &lt;!&ndash; admin-core  &ndash;&gt;-->
